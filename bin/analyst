#!/usr/bin/env bash
bin=`dirname $0`
bin=`cd "$bin"; pwd`
ANALYST_HOME=`cd "$bin"/..;pwd`
echo $ANALYST_HOME
readonly MASTER_JARS="$(ls "$ANALYST_HOME"/analyst-*.jar 2> /dev/null | tr "\n" :)"
CLASSPATH="$ANALYST_HOME/lib/*:$MASTER_JARS"
if [ ! $1 ]; then
  export DT=`date +%Y-%m-%d`
else
  export DT=$1
fi
echo $DT
java -Duser.timezone=Asia/Shanghai -DANALYST_HOME=${ANALYST_HOME} -Dlogback.configurationFile=$ANALYST_HOME/logback.xml -cp "$CLASSPATH" rocks.pho.stock.analyst -d $DT > $ANALYST_HOME/log/analyst.out 2>&1
